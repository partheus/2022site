            <a href="{{ post.url }}" class="irevamp-text">{{ post.data.title }}</a>
            <p style="margin-top: 5px" class="irevamp-lead">{{ post.data.date | postDate  }} {% if post.data.description %} â€¢ {{ post.data.description }}</p>{% endif %}
        </li>
      {%- endfor -%}
</ul>
</div>
